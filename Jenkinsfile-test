pipeline {
  triggers {
    cron('* * * * *')
  }

  agent any

  stages {
    stage('Run') {
      agent {
          docker {
              label 'docker'
              image 'python:2-alpine'
          }
      }
      steps {
        script {
          sh 'python print.py'
        }
      }
    }
  }
}
